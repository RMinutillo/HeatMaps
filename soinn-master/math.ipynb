   "source": [
    "from sympy import symbols, Eq, solve\n",
    "\n",
    "# Definir las variables simbólicas\n",
    "x = symbols('x')\n",
    "\n",
    "# Definir la ecuación\n",
    "equation = Eq(x**2 - 4, 0)\n",
    "\n",
    "# Resolver la ecuación\n",
    "solution = solve(equation, x)\n",
    "\n",
    "# Mostrar la solución\n",
    "print(\"Solución:\", solution)\n"
   ]
  }
